{
    "builds": [
      {
        "src": "hallucination_project/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize":"250mb","runtime":"python3.10"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "hallucination_project/wsgi.py"
      }
    ]
  }